<section data-component="multicolumn-listing">
    <div class="component-multicolumn-listing {{titleStateClass headingTitle}}">
        <div class="{{containerClasses width=width}}">    
            {{> linked-heading title=headingTitle isAlwaysLight=headingIsAlwaysLight ctaLink=headingCtaLink ctaNewWindow=headingCtaNewWindow ctaText=headingCtaText }}
            {{{multicolumnGrid}}}
        </div>
    </div>
    {{#each modalData}}
        {{#> modal-content uniqueID=uniqueID titleID='card-modal'}}
            {{> embed-video isVertical=isVertical classes=classes videoId=videoId noAutoPlay=noAutoPlay title=title}}
        {{/ modal-content}}
    {{/each}}
</section>