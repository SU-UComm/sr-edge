<section data-component='subtopic-subnav-component'>
    <div class='{{containerClasses width=width}}'>
        <div class='listing__header'>
            {{~#if showTopLevel~}}
                {{~#ifEquals isTopLevel 'false'~}}
                    <p
                        class='listing__back-link su-flex su-justify-center su-mb-15 md:su-mb-18 lg:su-mb-10'
                    >
                        <a
                            href={{parent.url}}
                            class='su-flex su-font-semi-bold su-items-center su-no-underline su-text-16 lg:su-text-18 su-text-black-60 hover:su-underline hover:su-text-digital-red dark:hover:su-text-dark-mode-red su-ml-[-.5rem] lg:su-ml-[-2rem]'
                        >
                            <svg
                                width='24'
                                height='24'
                                viewBox='0 0 24 24'
                                fill='none'
                                xmlns='http://www.w3.org/2000/svg'
                            >
                                <path
                                    fillRule='evenodd'
                                    clipRule='evenodd'
                                    d='M15.2485 6.35147C15.7172 6.8201 15.7172 7.5799 15.2485 8.04853L11.2971 12L15.2485 15.9515C15.7172 16.4201 15.7172 17.1799 15.2485 17.6485C14.7799 18.1172 14.0201 18.1172 13.5515 17.6485L8.75147 12.8485C8.28284 12.3799 8.28284 11.6201 8.75147 11.1515L13.5515 6.35147C14.0201 5.88284 14.7799 5.88284 15.2485 6.35147Z'
                                />
                            </svg>
                            <span>{{parent.title}}</span>
                        </a>
                    </p>
                {{~/ifEquals~}}
            {{~/if~}}
            {{~#if title~}}
                <h1
                    class='su-text-black su-font-bold su-font-serif dark:su-text-white su-text-center su-text-[39px] md:su-text-[54px] lg:su-text-[70px] su-leading-[46.57px] md:su-leading-[64.8px] lg:su-leading-[84px] su-mb-0'
                >
                    {{title}}
                </h1>
            {{~/if~}}
            <div data-list='subnav'></div>
        </div>
    </div>
</section>