<iframe
    width="{{#if isVertical}}315{{else}}560{{/if}}"
    height="{{#if isVertical}}560{{else}}315{{/if}}"
    class="{{classes}}"
    src="https://www.youtube.com/embed/{{videoId}}?si=vYU81uVmaV7GSju2&amp;autoplay={{#if noAutoPlay}}0{{else}}1{{/if}}&amp;controls=1&amp;rel=0"
    title="{{title}}"
    frameBorder="0"
    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
    allowfullscreen="true"
    data-modal="iframe"
    loading="lazy"
></iframe>
